ut</label>\n  </div>\n  <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n  \n</form>\n    </div>\n</template>\n<script>\nimport BaseRequest from \"../core/BaseRequest\";\nimport 'jquery/dist/jquery.min.js';\nimport $ from 'jquery';\nexport default {\n    data() {\n        return {\n            user: {\n                email: '',\n                name: '',\n                password: '',\n                password_confirmation:''\n                \n            },\n             errors:{},\n        }\n    },\n    mounted: function(){\n         \n        },\n    \n    methods: {\n        register: function() {\n            BaseRequest.post('register',this.user)\n            .then(response =>{\n                document.getElementById(\"successRegister\").style.display = \"block\";\n                $(\"#successRegister\").slideDown(100);\n                $(\"#successRegister\").slideUp(1000);\n                console.log(response);\n               BaseRequest.post('login',this.user)\n                    .then(response=> {\n                            window.localStorage.setItem('token', response.data.token);\n                            this.$router.push({name:'HomePage'});\n                  \n            })\n            })\n            .catch(error => {\n                this.errors = error.response.data.errors;\n                console.log(this.errors);\n            })\n        },\n        \n    }\n}\n</script>\n<style>\n#registerForm {\n    width: 500px;\n}\n#registerForm{\n    position: fixed;\n    top:30%;\n    left: 30%;\n    background-color:rgba(24,283,244,1);\n}\n</style>"],"sourceRoot":"src/components"}]}