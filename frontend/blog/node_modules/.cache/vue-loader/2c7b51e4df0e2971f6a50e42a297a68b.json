equest.post('register',this.user)\n            .then(response =>{\n                document.getElementById(\"successRegister\").style.display = \"block\";\n                $(\"#successRegister\").slideDown(100);\n                $(\"#successRegister\").slideUp(1000);\n                console.log(response);\n               BaseRequest.post('login',this.user)\n                    .then(response=> {\n                            window.localStorage.setItem('token', response.data.token);\n                            this.$router.push({name:'HomePage'});\n                  \n            })\n            })\n            .catch(error => {\n                this.errors = error.response.data.errors;\n                console.log(this.errors);\n            })\n        },\n        \n    }\n}\n</script>\n<style>\n#registerForm {\n    width: 500px;\n}\n#registerForm{\n    position: fixed;\n    top:30%;\n    left: 30%;\n    background-color:rgba(24,283,244,1);\n}\n</style>"]}]}